#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
     int a,b,c,d;
    scanf("%d%d",&a,&b);
    c=b;
    d=a;
    printf("The First value before Swapping:%d\nThe Second value before Swapping:%d\nThe First value after Swapping:%d\nThe Second value after Swapping:%d",a,b,c,d);
    return 0;
}
